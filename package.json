    "@astrojs/preact": "^2.0.3",
    "@babel/core": "^7.18.10",
    "@docsearch/css": "^3.2.2",
    "@docsearch/react": "^3.2.0",
    "@types/bun": "^1.0.0",
    "@types/canvas-confetti": "^1.6.0",
