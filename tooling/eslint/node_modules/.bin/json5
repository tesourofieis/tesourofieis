#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules/json5/lib/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules/json5/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/node_modules:/home/ofrades/dev/tesourofieis/node_modules:/home/ofrades/dev/node_modules:/home/ofrades/node_modules:/home/node_modules:/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules/json5/lib/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules/json5/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/eslint/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/tooling/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/node_modules:/home/ofrades/dev/tesourofieis/node_modules:/home/ofrades/dev/node_modules:/home/ofrades/node_modules:/home/node_modules:/node_modules:/home/ofrades/dev/tesourofieis/tesourofieis/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json5/lib/cli.js" "$@"
else
  exec node  "$basedir/../json5/lib/cli.js" "$@"
fi
